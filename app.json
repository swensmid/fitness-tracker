{
  "expo": {
    "name": "FitnessTracker",
    "slug": "FitnessTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ft.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ft.png",
        "backgroundColor": "#ffffff"
      },
      "package": "${PACKAGE_NAME}"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "${PROJECT_ID}"
      }
    },
    "owner": "${OWNER}"
  }
}
